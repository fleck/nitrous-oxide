{
  "version": "0.13.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fleck/nitrous-oxide"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --format cjs,esm,umd",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "name": "nitrous-oxide",
  "author": "Jonathan Fleckenstein",
  "module": "dist/nitrous-oxide.esm.js",
  "size-limit": [
    {
      "path": "dist/nitrous-oxide.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nitrous-oxide.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3"
  },
  "dependencies": {
    "@hotwired/turbo": "^7.0.0-beta.4",
    "@types/lodash": "^4.14.166",
    "delegate-it": "^2.0.2",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "mem": "^8.0.0"
  },
  "resolutions": {
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  }
}
